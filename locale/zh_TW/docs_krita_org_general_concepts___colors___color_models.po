#: ../../general_concepts/colors/color_models.rst:111
msgid ""
"There's also a difference between 'colored gray' and 'neutral gray' "
"depending on the profile."
msgstr ""

#: ../../general_concepts/colors/color_models.rst:115
msgid "25%"
msgstr ""

#: ../../general_concepts/colors/color_models.rst:115
msgid "50%"
msgstr ""

#: ../../general_concepts/colors/color_models.rst:115
